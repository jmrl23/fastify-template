{
  "version": "1.0.0",
  "name": "fastify-template",
  "repository": "https://github.com/jmrl23/fastify-template",
  "author": "Jomariel Gaitera <gaiterajomariel@gmail.com>",
  "private": true,
  "license": "MIT",
  "main": "build/main.js",
  "scripts": {
    "build": "tsc --build",
    "test": "dotenv -v NODE_ENV=test -- ts-node -r ./src/init.ts src/test.ts",
    "start": "node -r ./build/init.js .",
    "start:dev": "nodemon --inspect=9229 -q",
    "start:prod": "dotenv -v NODE_ENV=production -- node -r ./build/init.js .",
    "format": "prettier --write \"src/**/*.{ts,js}\"",
    "lint": "eslint"
  },
  "devDependencies": {
    "@swc/core": "^1.11.24",
    "@tsconfig/recommended": "^1.0.7",
    "@types/http-errors": "^2.0.4",
    "@types/node": "^22.15.3",
    "eslint": "^9.25.1",
    "globals": "^16.0.0",
    "json-schema-to-ts": "^3.1.1",
    "nodemon": "^3.1.10",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.1"
  },
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "@fastify/etag": "^6.0.0",
    "@fastify/middie": "^9.0.2",
    "@fastify/static": "^8.1.1",
    "@fastify/swagger": "^9.5.0",
    "@fastify/swagger-ui": "^5.2.2",
    "cache-manager": "^6.4.2",
    "detect-port": "^2.1.0",
    "dotenv": "^16.5.0",
    "dotenv-cli": "^8.0.0",
    "env-var": "^7.5.0",
    "fastify": "^5.3.2",
    "fastify-plugin": "^5.0.1",
    "glob": "^11.0.2",
    "http-errors": "^2.0.0",
    "keyv": "^5.3.3",
    "pino-pretty": "^13.0.0"
  }
}
